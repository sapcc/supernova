{
  "categories": [
    {
      "area": "landscape",
      "name": "All",
      "match_re": {
        "region": "^(?:.*)$"
      }
    },
    {
      "area": "landscape",
      "name": "Productive",
      "match_re": {
        "region": "^(?!staging|admin|qa-de-[2-9]+)"
      },
      "active": true
    },
    {
      "area": "landscape",
      "name": "Staging",
      "match_re": {
        "region": "^(?:staging|qa-de-[2-9]+|admin)$"
      }
    },
    {
      "area": "support",
      "name": "API",
      "match_re": {
        "tier": "^(?:os|k8s)$"
      }
    },
    {
      "area": "support",
      "name": "Metal",
      "match_re": {
        "tier": "^(?:metal)$"
      }
    },
    {
      "area": "support",
      "name": "Network",
      "match_re": {
        "tier": "^(?:net)$"
      }
    },
    {
      "area": "support",
      "name": "VMware",
      "match_re": {
        "tier": "^(?:vmware)$"
      }
    },
    {
      "area": "service",
      "name": "Concourse",
      "match_re": {
        "service": "^(?:concourse)$"
      }
    },
    {
      "area": "service",
      "name": "k8s",
      "match_re": {
        "tier": "^(?:k8s)$"
      }
    },
    {
      "area": "service",
      "name": "Kubernikus",
      "match_re": {
        "tier": "^(?:kks)$"
      }
    }
